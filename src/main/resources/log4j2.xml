<!--?xml version="1.0" encoding="UTF-8" standalone="no"?-->

<configuration monitorinterval="1800" status="off">
    <appenders>
    <console name="Console" target="SYSTEM_OUT">
        <patternlayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n">
        </patternlayout>
    </console>
</appenders>
<loggers>
     <root level="debug">
     </root>
</loggers>
</configuration>
