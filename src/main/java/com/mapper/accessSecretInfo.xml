<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bean.AccessSecretInfo">
    <select id="getAccessByAppid" parameterType="string" resultType="AccessSecretInfo">
        select * from `app_secret_info` where appid = #{appid}
    </select>
</mapper>