<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bean.AppSendcoinList">
    <insert id="add" keyProperty="id" useGeneratedKeys="true"  parameterType="com.bean.AppSendcoinList" >
        INSERT INTO `app_sendcoin_list` (`user_id`, `app_id`, `amount`,`coin`,`tag`,`description`, `create_time`)VALUES (#{user_id}, #{app_id}, #{amount},#{coin},#{tag},#{description},#{create_time});
    </insert>
</mapper>