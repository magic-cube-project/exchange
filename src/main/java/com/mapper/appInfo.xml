<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bean.AppInfo">
    <select id="select" parameterType="com.bean.AppInfo" resultType="com.bean.AppInfo">
        select * from `app_info` where app_id = #{app_id};
    </select>
</mapper>